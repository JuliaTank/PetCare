<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.petcare">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.example.permission.MAPS_RECEIVE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PetCare">
        <activity android:name=".activities.SignUpActivity"></activity>
        <activity
            android:name=".activities.OwnedOffers"
            android:parentActivityName=".activities.MainActivity2" />
        <activity
            android:name=".activities.MainActivity5"
            android:parentActivityName=".activities.MainActivity2" />
        <activity android:name=".activities.MainActivity4" />
        <activity
            android:name=".activities.MainActivity3"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.MainActivity2"
            android:parentActivityName=".activities.MainActivity3" />
        <activity
            android:name=".activities.MainActivity"
            android:parentActivityName=".activities.MainActivity3">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>